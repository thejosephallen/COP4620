X    {{{}}{int x;}}
X    {x(x);}
X    if (1) return x[1];
X    if (null) {
X        x = x; 1 + 2;
X        x + 1 + 2 / 3;
X    } else return 0;
X    while (0 <= x[2])
X        print(hello(world(x,x,z-z/z), x));
}
//-------------------------------------------------
/*
int gcd(int u, int v){
X    if (v == 0) return u;
X    else return gcd(v,u-u/v*v);
X    /* u-u/v*v == u mod v */
}
void main(void){
X    int x; int y;
X    x = input(); y = input();
X    output(gcd(x,y));
}
*/
//------------------------------------------------
/*
int x[10];
int minloc ( int a[], int low, int high ){
X    int i; int x; int k;
X    k = low;
X    x = a[low];
X    i = low + 1;
X    while ( i < high ){
X        if (a[i] < x){
X            x = a[i];
X            k = i;
X        }
X        i = i + 1;
X    }
X    return k;
}
X
void sort( int a[], int low, int high){
X    int i; int k;
X    i = low;
X    while ( i < high -1){
X        int t;
X        k = minloc(a,i,high);
X        t = a[k];
X        a[k] = a[i];
X        a[i] = t;
X        i = i + 1;
X    }
}
X
void main(void){
X    int i;
X    i = 0;
X    while (i < 10){
X        x[i] = input();
X        i = i + 1;
X    }
X    sort(x, 0, 10);
X    i = 0;
X    while (i < 10){
X        output(x[i]);
X        i = i + 1;
X    }
}
*/
X
