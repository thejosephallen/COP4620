#!/bin/sh
# This is a shell archive (produced by GNU sharutils 4.15.2).
# To extract the files from this archive, save it to some FILE, remove
# everything before the '#!/bin/sh' line above, then type 'sh FILE'.
#
lock_dir=_sh06749
# Made on 2018-10-20 14:53 EDT by <joseph@msi>.
# Source directory was '/home/joseph/Desktop/COP4620 Compilers/Project2'.
#
# Existing files will *not* be overwritten, unless '-c' is specified.
#
# This shar contains:
# length mode       name
# ------ ---------- ------------------------------------------
#    475 -rw-r--r-- doc.txt
#     27 -rwxrwxrwx p2
#   2177 -rw-rw-r-- parse.py
#   3977 -rw-rw-r-- tools.py
#   2625 -rw-r--r-- grammar
#   3297 -rw-r--r-- p1.py
#   1021 -rw-rw-r-- p2.py
#   1237 -rw-rw-r-- testfiles
#    379 -rw-r--r-- typescript
#   1895 -rw-r--r-- first_sets
#     63 -rw-rw-r-- makefile
#
MD5SUM=${MD5SUM-md5sum}
f=`${MD5SUM} --version | egrep '^md5sum .*(core|text)utils'`
test -n "${f}" && md5check=true || md5check=false
${md5check} || \
  echo 'Note: not verifying md5sums.  Consider installing GNU coreutils.'
if test "X$1" = "X-c"
then keep_file=''
else keep_file=true
fi
echo=echo
save_IFS="${IFS}"
IFS="${IFS}:"
gettext_dir=
locale_dir=
set_echo=false

for dir in $PATH
do
  if test -f $dir/gettext \
     && ($dir/gettext --version >/dev/null 2>&1)
  then
    case `$dir/gettext --version 2>&1 | sed 1q` in
      *GNU*) gettext_dir=$dir
      set_echo=true
      break ;;
    esac
  fi
done

if ${set_echo}
then
  set_echo=false
  for dir in $PATH
  do
    if test -f $dir/shar \
       && ($dir/shar --print-text-domain-dir >/dev/null 2>&1)
    then
      locale_dir=`$dir/shar --print-text-domain-dir`
      set_echo=true
      break
    fi
  done

  if ${set_echo}
  then
    TEXTDOMAINDIR=$locale_dir
    export TEXTDOMAINDIR
    TEXTDOMAIN=sharutils
    export TEXTDOMAIN
    echo="$gettext_dir/gettext -s"
  fi
fi
IFS="$save_IFS"
if (echo "testing\c"; echo 1,2,3) | grep c >/dev/null
then if (echo -n test; echo 1,2,3) | grep n >/dev/null
     then shar_n= shar_c='
'
     else shar_n=-n shar_c= ; fi
else shar_n= shar_c='\c' ; fi
f=shar-touch.$$
st1=200112312359.59
st2=123123592001.59
st2tr=123123592001.5 # old SysV 14-char limit
st3=1231235901

if   touch -am -t ${st1} ${f} >/dev/null 2>&1 && \
     test ! -f ${st1} && test -f ${f}; then
  shar_touch='touch -am -t $1$2$3$4$5$6.$7 "$8"'

elif touch -am ${st2} ${f} >/dev/null 2>&1 && \
     test ! -f ${st2} && test ! -f ${st2tr} && test -f ${f}; then
  shar_touch='touch -am $3$4$5$6$1$2.$7 "$8"'

elif touch -am ${st3} ${f} >/dev/null 2>&1 && \
     test ! -f ${st3} && test -f ${f}; then
  shar_touch='touch -am $3$4$5$6$2 "$8"'

else
  shar_touch=:
  echo
  ${echo} 'WARNING: not restoring timestamps.  Consider getting and
installing GNU '\''touch'\'', distributed in GNU coreutils...'
  echo
fi
rm -f ${st1} ${st2} ${st2tr} ${st3} ${f}
#
if test ! -d ${lock_dir} ; then :
else ${echo} "lock directory ${lock_dir} exists"
     exit 1
fi
if mkdir ${lock_dir}
then ${echo} "x - created lock directory ${lock_dir}."
else ${echo} "x - failed to create lock directory ${lock_dir}."
     exit 1
fi
# ============= doc.txt ==============
if test -n "${keep_file}" && test -f 'doc.txt'
then
${echo} "x - SKIPPING doc.txt (file already exists)"

else
${echo} "x - extracting doc.txt (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'doc.txt' &&
This program performs the functions of a parser for a C- language.
X
Components:
X	p1.py - scanner from Project 1 that obtains a token stack
X	p2.py - driver code for Project 2 that calls p1 and other necessary functions
X	parse.py - program that does the actual parsing given the context of the problem
X	tools.py - contains functions to load in the grammar and first sets and compute the follow sets
X
Input: source program to parse, grammar, first sets
Output: ACCEPT or REJECT
SHAR_EOF
  (set 20 18 10 20 14 47 33 'doc.txt'
   eval "${shar_touch}") && \
  chmod 0644 'doc.txt'
if test $? -ne 0
then ${echo} "restore of doc.txt failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'doc.txt': 'MD5 check failed'
       ) << \SHAR_EOF
5d5bbbe9738e691a70ccfdf8f5ac6465  doc.txt
SHAR_EOF

else
test `LC_ALL=C wc -c < 'doc.txt'` -ne 475 && \
  ${echo} "restoration warning:  size of 'doc.txt' is not 475"
  fi
fi
# ============= p2 ==============
if test -n "${keep_file}" && test -f 'p2'
then
${echo} "x - SKIPPING p2 (file already exists)"

else
${echo} "x - extracting p2 (text)"
  sed 's/^X//' << 'SHAR_EOF' | uudecode &&
begin 600 p2
;(R$O8FEN+V)A<V@*<'ET:&]N('`R+G!Y("0Q
`
end
SHAR_EOF
  (set 20 18 10 19 16 44 14 'p2'
   eval "${shar_touch}") && \
  chmod 0777 'p2'
if test $? -ne 0
then ${echo} "restore of p2 failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'p2': 'MD5 check failed'
       ) << \SHAR_EOF
19ee2104fcf0fde92c569628fcc9a5d3  p2
SHAR_EOF

else
test `LC_ALL=C wc -c < 'p2'` -ne 27 && \
  ${echo} "restoration warning:  size of 'p2' is not 27"
  fi
fi
